
<h1 id="head-form">Log in</h1>
<div id ="athentification-form">
 <%= bootstrap_form_for :session , url: login_path do |f| %>
  <%= f.email_field :email %>
  <%= f.password_field :password %>
  <%= f.check_box :remember_me, checked: true%>
  <%= link_to "forget password?", new_password_reset_path %>
  <%= f.submit "Log in" %>
 <% end %>
</div>